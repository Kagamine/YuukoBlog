<div class="sidebar-module sidebar-catalogs">
    <h3>TAGS</h3>
    <div class="sidebar-body"><ul class="sidebar-list">
            <% tags.forEach(x => { %>
            <% if (x.tag) { %>
            <li class="sidebar-item"><a href="/tag/<%= x.tag %>/"><%= x.tag %> (<%= x.count %>)</a></li>
            <% } }) %>
        </ul></div>
</div>